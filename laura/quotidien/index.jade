div.checklist
	h1 Laura au quotidien

	div(id="data" data-enfant="laura")

	aside.col-3
		div#laura
			div.jauge
			div.verre
			div.ombre
			div.light
			div.reflet1
			div.reflet2

	aside.col-6
		for enfant, slug in laura.checklists.quotidien
			div(class="checklist")
				div.row #{ slug } - #{ enfant }
					input(type="checkbox" class="js-switch" data-jauge="#{ enfant }")